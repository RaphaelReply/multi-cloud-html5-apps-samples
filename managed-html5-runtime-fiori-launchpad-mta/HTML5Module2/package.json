{
    "name": "demo",
    "version": "0.0.1",
    "scripts": {
        "start": "ui5 serve --config=ui5.yaml  --open index.html",
        "build": "npm run clean && npm run build:ui5 && npm run zip",
        "clean": "npx rimraf HTML5Module2-content.zip",
        "build:ui5": "ui5 build -a",
        "zip": "cd dist && npx bestzip ../HTML5Module2-content.zip *"
    },
    "devDependencies": {
        "@ui5/cli": "^2.14.1",
        "chart.js": "^3.8.0",
        "ui5-task-zipper": "^0.4.7",
        "ui5-tooling-modules": "^0.3.3"
    },
    "ui5": {
        "dependencies": [
            "ui5-task-zipper",
            "ui5-tooling-modules"
        ]
    }
}
